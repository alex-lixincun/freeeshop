
@model Final_eshop_xincunli.Models.Member

<h3>@(Model.Name) personal information</h3>


<div class="display-field">
    <span class="label">@Html.DisplayNameFor(model => model.Name):</span>
    @Html.DisplayFor(model => model.Name)
</div>

<div class="display-field">
    <span class="label">@Html.DisplayNameFor(model => model.Role):</span>
    @Html.DisplayFor(model => model.Role.ToString())
</div>

<div class="display-field">
    <span class="label">@Html.DisplayNameFor(model => model.RegisterDate)</span><br />
    @Html.DisplayFor(model => model.RegisterDate)
</div>


